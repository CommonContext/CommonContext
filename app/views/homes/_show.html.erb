<div class="choice-container">	
	<article class="row center">
		<div class="registration">
		<section class="card hoverable col s6">
		<h3>Complete Registration</h3>
			<% if current_user %>
				<ul class="center-align">
					<li class="sign-up"><%= link_to "Sign up as Mentor", new_mentor_path, class: "btn" %></li>
					<li class="sign-up"><%= link_to "Sign up as Mentee", new_mentee_path, class: "btn" %></li>	
				</ul>
			<% else %>
			<p>Oops! You've already finished registration.</p>
			<% end %>
		</section>
		</div>
	</article>
</div>
